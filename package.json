{
  "name": "ts-mocker",
  "type": "module",
  "version": "0.1.5",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "description": "ts-mocker is a performance-focused and lightweight faker library for TypeScript.",
  "author": "Chris Breuer <chris@stacksjs.org>",
  "license": "MIT",
  "homepage": "https://github.com/stacksjs/ts-mocker#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/stacksjs/ts-mocker.git"
  },
  "bugs": {
    "url": "https://github.com/stacksjs/ts-mocker/issues"
  },
  "keywords": [
    "faker",
    "fake-data",
    "test-data",
    "mock-data",
    "typescript",
    "locale",
    "i18n",
    "internationalization",
    "seeding",
    "testing",
    "fixtures",
    "random",
    "generator",
    "mock",
    "bun",
    "performance",
    "lightweight",
    "cli"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/src/index.js"
    },
    "./*": {
      "import": "./dist/*"
    }
  },
  "module": "./dist/src/index.js",
  "types": "./dist/index.d.ts",
  "bin": {
    "ts-mocker": "./packages/core/dist/bin/cli.js",
    "mock": "./packages/core/dist/bin/cli.js"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "scripts": {
    "build": "bun run scripts/build-all.ts",
    "build:old": "bun --bun build.ts && bun run compile",
    "compile": "bun build ./bin/cli.ts --compile --minify --outfile bin/mock",
    "compile:all": "bun run compile:linux-x64 && bun run compile:linux-arm64 && bun run compile:windows-x64 && bun run compile:darwin-x64 && bun run compile:darwin-arm64",
    "compile:linux-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-linux-x64 --outfile bin/mock-linux-x64",
    "compile:linux-arm64": "bun build ./bin/cli.ts --compile --minify --target=bun-linux-arm64 --outfile bin/mock-linux-arm64",
    "compile:windows-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-windows-x64 --outfile bin/mock-windows-x64.exe",
    "compile:darwin-x64": "bun build ./bin/cli.ts --compile --minify --target=bun-darwin-x64 --outfile bin/mock-darwin-x64",
    "compile:darwin-arm64": "bun build ./bin/cli.ts --compile --minify --target=bun-darwin-arm64 --outfile bin/mock-darwin-arm64",
    "zip": "bun run zip:all",
    "zip:all": "bun run zip:linux-x64 && bun run zip:linux-arm64 && bun run zip:windows-x64 && bun run zip:darwin-x64 && bun run zip:darwin-arm64",
    "zip:linux-x64": "zip -j bin/mock-linux-x64.zip bin/mock-linux-x64",
    "zip:linux-arm64": "zip -j bin/mock-linux-arm64.zip bin/mock-linux-arm64",
    "zip:windows-x64": "zip -j bin/mock-windows-x64.zip bin/mock-windows-x64.exe",
    "zip:darwin-x64": "zip -j bin/mock-darwin-x64.zip bin/mock-darwin-x64",
    "zip:darwin-arm64": "zip -j bin/mock-darwin-arm64.zip bin/mock-darwin-arm64",
    "fresh": "bunx rimraf node_modules/ bun.lock && bun i",
    "prepublishOnly": "bun --bun run build && bun run compile:all && bun run zip",
    "test": "bun test",
    "bench": "cd packages/benchmarks && bun run bench",
    "lint": "bunx --bun eslint .",
    "lint:fix": "bunx --bun eslint . --fix",
    "changelog": "bunx logsmith --verbose",
    "changelog:generate": "bunx logsmith --output CHANGELOG.md",
    "release": "bun run changelog:generate && bunx --bun bumpx prompt --recursive",
    "dev:docs": "bun --bun vitepress dev docs",
    "build:docs": "bun --bun vitepress build docs",
    "preview:docs": "bun --bun vitepress preview docs",
    "typecheck": "bun --bun tsc --noEmit"
  },
  "devDependencies": {
    "@stacksjs/bumpx": "^0.2.2",
    "@stacksjs/docs": "^0.70.23",
    "@stacksjs/eslint-config": "^4.14.0-beta.3",
    "@stacksjs/gitlint": "^0.1.5",
    "@stacksjs/logsmith": "^0.2.0",
    "@types/bun": "^1.3.1",
    "buddy-bot": "^0.9.9",
    "bun-plugin-dtsx": "0.21.12",
    "bunfig": "^0.15.1",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "unconfig": "0.3.10"
  }
}
